{
  "expo": {
    "name": "E-Donor",
    "slug": "e-donor",
    "version": "2.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "edonor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarStyle": "UIStatusBarStyleDarkContent"
      },
      "bundleIdentifier": "com.mrraviya.EDonor"
    },
    "android": {
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.mrraviya.EDonor"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "translucent": true
    },
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden"
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "abiFilters": [
              "arm64-v8a"
            ],
            "enableProguardInReleaseBuilds": true,
            "hermesEnabled": true,
            "enableMinifyInReleaseBuilds": true
          },
          "ios": {
            "hermesEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden"
      },
      "eas": {
        "projectId": "b0a1fd12-d027-4b7e-b7dc-0cf139114c38"
      },
      "firebase": {
        "apiKey": "AIzaSyD7mFH0jX9SoxH1JCzXfWu9daPBjzTSBdU",
        "authDomain": "e-donor-2025.firebaseapp.com",
        "projectId": "e-donor-2025",
        "storageBucket": "e-donor-2025.firebasestorage.app",
        "messagingSenderId": "326352899654",
        "appId": "1:326352899654:web:6aa651d308c8a06e1e8447"
      }
    },
    "owner": "mrraviya"
  }
}
